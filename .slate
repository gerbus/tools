# Configuration file for Slate
#  https://github.com/jigish/slate
#  https://blog.mikecordell.com/2014/05/25/getting-started-with-slate.html


# Configs
config defaultToCurrentScreen true
config nudgePercentOf screenSize
config resizePercentOf screenSize
config secondsBetweenRepeat 0.1
config checkDefaultsOnLoad true
config focusCheckWidthMax 3000


# Monitor Aliases
alias m-left		  0
alias m-right 		1
alias m-90        2


# Position Aliases
alias quarter         0.25
alias third 	        0.4
alias half 		        0.5
alias twothirds       0.6
alias threequarters   0.75
alias fat 		        0.8
alias nearfull        0.95

alias left-third		          move screenOriginX;screenOriginY								                screenSizeX*${third};screenSizeY
alias left-half 		          move screenOriginX;screenOriginY								                screenSizeX*${half};screenSizeY
alias left-twothirds 	        move screenOriginX;screenOriginY								                screenSizeX*${twothirds};screenSizeY
alias left-twothirds-2 	      move screenOriginX+50;screenOriginY								                screenSizeX*${twothirds};screenSizeY
alias left-twothirds-3 	      move screenOriginX+100;screenOriginY								                screenSizeX*${twothirds};screenSizeY
alias left-twothirds-4 	      move screenOriginX+150;screenOriginY								                screenSizeX*${twothirds};screenSizeY
alias left-twothirds-5 	      move screenOriginX+200;screenOriginY								                screenSizeX*${twothirds};screenSizeY
alias left-threequarters 	    move screenOriginX;screenOriginY								                screenSizeX*${threequarters};screenSizeY
alias left-threequarters-2 	  move screenOriginX+50;screenOriginY								                screenSizeX*${threequarters};screenSizeY
alias left-threequarters-3 	  move screenOriginX+100;screenOriginY								                screenSizeX*${threequarters};screenSizeY
alias left-threequarters-4 	  move screenOriginX+150;screenOriginY								                screenSizeX*${threequarters};screenSizeY
alias left-fat 			          move screenOriginX;screenOriginY								                screenSizeX*${fat};screenSizeY
alias left-fat-2 		          move screenOriginX+40;screenOriginY								                screenSizeX*${fat};screenSizeY
alias left-fat-3 		          move screenOriginX+80;screenOriginY								                screenSizeX*${fat};screenSizeY
alias left-fat-4 		          move screenOriginX+120;screenOriginY								                screenSizeX*${fat};screenSizeY
alias left-fat-5 		          move screenOriginX+160;screenOriginY								                screenSizeX*${fat};screenSizeY
alias left-fat-6 		          move screenOriginX+200;screenOriginY								                screenSizeX*${fat};screenSizeY
alias left-fat-7 		          move screenOriginX+240;screenOriginY								                screenSizeX*${fat};screenSizeY
alias left-fat-8 		          move screenOriginX+280;screenOriginY								                screenSizeX*${fat};screenSizeY
alias left-fat-9 		          move screenOriginX+320;screenOriginY								                screenSizeX*${fat};screenSizeY
alias left-fat-10 		        move screenOriginX+360;screenOriginY								                screenSizeX*${fat};screenSizeY
alias left-fat-11 		        move screenOriginX+400;screenOriginY								                screenSizeX*${fat};screenSizeY
alias left-fat-12 		        move screenOriginX+440;screenOriginY								                screenSizeX*${fat};screenSizeY
alias left-nearfull		    move screenOriginX;screenOriginY								                screenSizeX*${nearfull};screenSizeY
alias left-nearfull-2		    move screenOriginX+40;screenOriginY								                screenSizeX*${nearfull};screenSizeY
alias left-nearfull-3		    move screenOriginX+80;screenOriginY								                screenSizeX*${nearfull};screenSizeY
alias left-nearfull-4		    move screenOriginX+160;screenOriginY								                screenSizeX*${nearfull};screenSizeY
alias left-full-1         move screenOriginX;screenOriginY                             screenSizeX-50;screenSizeY
alias left-full-2         move screenOriginX;screenOriginY                             screenSizeX-100;screenSizeY
alias left-full-3         move screenOriginX;screenOriginY                             screenSizeX-150;screenSizeY
alias center-third		    move screenOriginX+screenSizeX*(1-${third})/2;screenOriginY		                screenSizeX*${third};screenSizeY
alias center-half		      move screenOriginX+screenSizeX*(1-${half})/2;screenOriginY		                screenSizeX*${half};screenSizeY
alias center-twothirds	  move screenOriginX+screenSizeX*(1-${twothirds})/2;screenOriginY	                screenSizeX*${twothirds};screenSizeY
alias center-fat 		      move screenOriginX+screenSizeX*(1-${fat})/2;screenOriginY		                screenSizeX*${fat};screenSizeY
