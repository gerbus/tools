alias full		            move screenOriginX;screenOriginY								                screenSizeX;screenSizeY
alias right-quarter 	    move screenOriginX+screenSizeX*(1-${quarter});screenOriginY		                screenSizeX*${quarter};screenSizeY
alias right-third 		    move screenOriginX+screenSizeX*(1-${third});screenOriginY		                screenSizeX*${third};screenSizeY
alias right-third-2 	    move screenOriginX+screenSizeX*(1-${third});screenOriginY		                screenSizeX*${third}-50;screenSizeY
