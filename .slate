alias right-twothirds	    move screenOriginX+screenSizeX*(1-${twothirds});screenOriginY	                screenSizeX*${twothirds};screenSizeY
alias right-twothirds-2	    move screenOriginX-50+screenSizeX*(1-${twothirds});screenOriginY	                screenSizeX*${twothirds};screenSizeY
alias right-twothirds-3	    move screenOriginX-100+screenSizeX*(1-${twothirds});screenOriginY	                screenSizeX*${twothirds};screenSizeY
alias right-twothirds-4	    move screenOriginX-150+screenSizeX*(1-${twothirds});screenOriginY	                screenSizeX*${twothirds};screenSizeY
alias right-twothirds-5	    move screenOriginX-200+screenSizeX*(1-${twothirds});screenOriginY	                screenSizeX*${twothirds};screenSizeY
alias right-twothirds-6	    move screenOriginX-250+screenSizeX*(1-${twothirds});screenOriginY	                screenSizeX*${twothirds};screenSizeY
alias right-threequarters move screenOriginX+screenSizeX*(1-${threequarters});screenOriginY	                screenSizeX*${threequarters};screenSizeY
alias right-fat 		      move screenOriginX+screenSizeX*(1-${fat});screenOriginY			                    screenSizeX*${fat};screenSizeY
