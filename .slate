# Default Layouts
default laptop-*ext							resolutions:${m-right};${m-left};


# Bind
bind 1:alt;cmd		      layout laptop
bind 2:alt;cmd 	        layout laptop-*ext      # "main" screen is the external one, and right of the laptop
bind 2:alt;cmd;shift    layout *laptop-ext      # "main" screen is the laptop (external on right)
bind 3:alt;cmd          layout laptop-*ext-90   # From left to right: laptop, main external, 90 degree orientation external

bind s:alt;cmd 		focus 'Sublime Text'
bind t:alt;cmd		focus 'iTerm2'
bind c:alt;cmd		focus 'Google Chrome'

bind right:alt;cmd	      chain ${right-half} | ${right-twothirds} | ${right-fat} | ${right-nearfull} | chain ${right-third}
bind left:alt;cmd	        chain ${left-half} | ${left-twothirds} | ${left-fat} | ${left-nearfull} | chain ${left-third}
bind up:alt;cmd		        chain ${center-half} | ${center-twothirds} | ${center-fat} | ${center-nearfull} | ${full}
bind down:alt;cmd	        throw next
bind right:alt;shift;cmd	nudge +50 +0
bind left:alt;shift;cmd		nudge -50 +0
