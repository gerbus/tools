layout laptop-*ext 'Activity Monitor'				          ${right-quarter} ${m-left}
layout laptop-*ext 'Sequel Pro':REPEAT 			          ${right-half} ${m-left}
layout laptop-*ext 'Slack'							              ${left-threequarters} ${m-left}
layout laptop-*ext 'Zoom'		                          ${full} ${m-left}
layout laptop-*ext 'qemu-system-x86_64'               ${right-third} ${m-left}
layout laptop-*ext 'Simulator'                        ${center-third} ${m-left}

layout laptop-*ext 'Sublime Text':REPEAT			        ${left-threequarters} ${m-right} | ${left-threequarters-2} ${m-right} | ${left-threequarters-3} ${m-right} | ${left-threequarters-4} ${m-right}
layout laptop-*ext 'Code':REPEAT			                ${left-threequarters} ${m-right} | ${left-threequarters-2} ${m-right} | ${left-threequarters-3} ${m-right} | ${left-threequarters-4} ${m-right}
