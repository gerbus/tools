# Montior Layouts


# flying solo (one display)
layout laptop 'Todoist':REPEAT		                ${left-half}
layout laptop 	'Slack'							              ${left-fat-2}
layout laptop 	'Google Chat'				              ${left-fat-2}
layout laptop 	'Google Chrome':REPEAT    	      ${left-fat-3} | ${left-fat-4} | ${left-fat-5} | ${left-fat-6} | ${left-fat-7} | ${left-fat-8}
layout laptop 'Zoom'		                          ${center-fat}
layout laptop 'Google Meet'                       ${center-fat}
layout laptop 'Google Calendar'                   ${center-fat}

layout laptop 'My Devs'                           ${right-nearfull}
layout laptop 'Lattice'                           ${right-nearfull}
layout laptop 	'Code':REPEAT 			              ${right-nearfull} | ${right-nearfull-2} | ${right-nearfull-3} | ${left-nearfull}
layout laptop 	'Cursor':REPEAT 			            ${right-nearfull} | ${right-nearfull-2} | ${right-nearfull-3} | ${left-nearfull}
